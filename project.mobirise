Color\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"wrapper\">\n            <div class=\"mbr-section-head\" mbr-if=\"showTitle\">\n                <h3 mbr-if=\"showTitle\" class=\"mbr-section-title mbr-fonts-style mb-0\" data-app-selector=\".mbr-section-title\" mbr-theme-style=\"display-2\">\n                    <b>Сourses</b>\n                </h3>\n            </div>\n            <div class=\"row mt-4\">\n                <div class=\"card col-12\">\n                    <div class=\"card-wrapper mb-5\">\n                        <div mbr-if=\"showInfo\">\n                            <div class=\"info b3 rounded-circle text-center text-white  d-flex align-items-center\">\n                                <h5 class=\"mbr-text mbr-fonts-style m-auto p-2\" mbr-theme-style=\"display-4\">Starts at<br>13<br>APRIL</h5>\n                            </div>\n                        </div>\n\n                        <div class=\"top-line mb-4\" mbr-if=\"showTitleCard\">\n                            <h4 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".card-title\">Web Designing</h4>\n                        </div>\n                        <div class=\"bottom-line row\">\n                            <div class=\"col-12 col-lg-5 mb-5 mb-lg-0\">\n                                <p class=\"mbr-text mbr-fonts-style m-0\" mbr-theme-style=\"display-7\" mbr-if=\"showText\">\n                                    Create front-end part of website using HTML,CSS,JAVASCRIPT,BOOTS